<html>
    <head>
        <title>Skills</title>
    </head>
    <body>
        <h1>Skills</h1>
        <table border="1">
            <tr><th>Skill ID</th><th>Name</th><th>Description</th></tr>
                <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><%= result[i].skill_id%></td>
                <td><a href="/skill/?skill_id=<%= result[i].skill_id %>"><%= result[i].skill_name %></a></td>
                <td><a href="/skill/?skill_id=<%= result[i].skill_id %>"><%= result[i].skill_description%></a></td>
            </tr>
            <% } %>
        </table>
    </body>
</html>